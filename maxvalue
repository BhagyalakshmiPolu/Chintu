//Write a program to find the maximum element in an array.//
#include <stdio.h>
int main() {
    int a[6],max,n,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0,max=a[0];i<n;i++)
    {
        if(max<a[i])//if(min<a[i])
        {
            max=a[i];//min=a[i]
        }
    }
    printf("%d ",max);//pf("%d",min)
}
