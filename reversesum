//reverse &sum the given number//
#include<stdio.h>
void main()
{
    int n,r,sum,temp;
    scanf("%d",&n);
    sum=0;
    temp=n;
    while(n!=0)
    {
        r=n%10;
        sum=sum*10+r;
        n=n/10;
    }
    n=temp;
    if(sum==n)
    {
        printf("palindrome");
    }
    else
    {
        printf("not a palindrome");
    }
}
