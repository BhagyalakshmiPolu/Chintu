//minimum and maximum in arrays//
#include<stdio.h>
void main()
{
    int a[6],max,min,n,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0,max=a[0],min=a[0];i<n;i++)
    {
        if(max<a[i])
        {
            max=a[i];
        }
        else{
             min=a[i];
        }
    }
    printf("%d ",max);
    printf("%d ",min);
}
