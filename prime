//given number pprime or not//
#include<stdio.h>
void main()
{
   int n,i;
   scanf("%d",&n);
   for(i=2;i*i<=n;i++)
   {
       if(n%i==0)
       {
           break;
       }
       if(i>n/2)
       {
        printf("\n %d is a primenumber",n);
       }
       else
       {
        printf("\n %d is  not a primenumber",n);

   }

}
}
