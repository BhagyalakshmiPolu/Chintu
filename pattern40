//print the pattern 40//
#include <stdio.h>
int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(i<=j)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
